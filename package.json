{
  "name": "swagger-builder",
  "version": "1.0.0",
  "description": "build swagger to javascript with axios",
  "author": "kaisawind <wind.kaisa@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build:swagger": "grunt vue"
  },
  "swagger": {
    "definition": "./example/swagger/swagger.json",
    "className": "API",
    "moduleName": "apis",
    "dist": "./example/dist"
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaisawind/swagger-builder.git"
  },
  "bugs": {
    "url": "https://github.com/kaisawind/swagger-builder/issues"
  },
  "dependencies": {
    "axios": "0.19.0"
  },
  "devDependencies": {
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.2",
    "handlebars": "^4.1.1",
    "js-beautify": "1.10.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
